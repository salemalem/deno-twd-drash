{
  "$schema": "https://deno.land/x/denon@2.4.8/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run -A server.js",
      "desc": "run my server.js file",
      "unstable": true
    }
  },
  "watcher": {
    "interval": 350,
    "exts": ["js", "eta", "html", "css"],
    "match": ["**/*.*"]
  },

  "logger": {
    "quiet": false,
    "debug": true
  }
}